    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "1.1.20",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.19.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
